<%- include('pages/layout') %>
    <div class="col-sm-10 border">
        <ul>
            <h1>
                <%= title %>
            </h1>
            <% category_list.forEach(function (item){ %>
                <li>
                    <strong>
                        <a href="categories/<%= item._id %>">
                            <%= item.name %>
                        </a>
                    </strong>
                    <p>
                        <%= item.description %>
                    </p>
                </li>
                <% }); %>
        </ul>
    </div>